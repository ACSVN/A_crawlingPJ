console.log('eg_dot');
// Engadget Smartphone DOT
function vmfireDotEvent(vm_property,vm_category){
window.dotq = window.dotq || [];
window.dotq.push(
{
'projectId': '10000',
'properties': {
'pixelId': '428726',
'qstrings': {
'et': 'custom',
'country':'JP',
'property':vm_property,
'category':vm_category
}
} } )
}; 

(function(){
	if (location.href.toLowerCase().indexOf('//japanese.engadget.com/')>-1){
		if (location.href.toLowerCase().indexOf('//japanese.engadget.com/special/sp2019-spring/')>-1){
 	 		var img=new Image();
			img.src='https://sp.analytics.yahoo.com/spp.pl?a=10000&.yp=428726&country=jp&property=engadget&category=smartphone,android,xperia,huawei,oppo,iij,docomo,au,softbank,biglobe,uqmobile,galaxy,samsung,mineo';
			vmfireDotEvent('engadget','smartphone,android,xperia,huawei,oppo,iij,docomo,au,softbank,biglobe,uqmobile,galaxy,samsung,mineo');
			console.log('Hubpage');
 		} else {
			try {
				var tacProp2=''
				var tags=document.getElementsByTagName('meta');
				for (i=0;i<tags.length;i++){
    				var attr=tags[i].getAttribute('property');
					if (attr=='article:tag'){
					    if (tags[i].content.length>0) {
		   				   tacProp2=tacProp2+tags[i].content+'|';		
                        }
					}
				}
				if (tacProp2!=''){tacProp2=tacProp2.substring(0,tacProp2.length-1)}
				if (tacProp2!=''){
					if(('|'+tacProp2+'|').match(/.*[|](smartphone|android|xperia|huawei|oppo|iij|docomo|au|softbank|biglobe|uqmobile|galaxy|samsung|mineo)[|].*/)!=null){
						var img=new Image();
						img.src='https://sp.analytics.yahoo.com/spp.pl?a=10000&.yp=428726&country=jp&property=engadget&category=smartphone,android,xperia,huawei,oppo,iij,docomo,au,softbank,biglobe,uqmobile,galaxy,samsung,mineo';
            			vmfireDotEvent('engadget','smartphone,android,xperia,huawei,oppo,iij,docomo,au,softbank,biglobe,uqmobile,galaxy,samsung,mineo');
						console.log(tacProp2);
					};
				}
			} catch(ex){}
		} 
	}
})()