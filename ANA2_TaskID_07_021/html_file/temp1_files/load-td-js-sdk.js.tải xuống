(function() {
  var getEnv = function(){
    var env = '';
    if (location.host.match(/(dev|develop|ib-test)/)) {
      env = 'dev';
    } else if (location.host.match(/(stg|staging)/)) {
      env = 'stg';
    } else {
      env = 'prd';
    }
    return env;
  };

