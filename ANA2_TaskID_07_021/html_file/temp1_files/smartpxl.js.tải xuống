function vm_insertScript(id,url){var dt = new Date();dt = dt.getFullYear().toString()+("00" + (dt.getMonth()+1)).slice(-2)+("00" + dt.getDate()).slice(-2);var s=document.createElement('script');s.src=url +'?rnd='+dt;s.id=id;s.async=true;document.body.appendChild(s);}


//var vm_isMobile=( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) 

  window.addEventListener("load", function(event) {
//call subpixels;
    vm_insertScript('eg_native','https:////bannerfarm.aolp.jp/smartpxl/00002/smp/eg_native.js');
    vm_insertScript('eg_dot','https:////bannerfarm.aolp.jp/smartpxl/00002/smp/eg_dot.js');
    vm_insertScript('eg_autohide','https:////bannerfarm.aolp.jp/smartpxl/00002/smp/eg_autohide2.js');  

});