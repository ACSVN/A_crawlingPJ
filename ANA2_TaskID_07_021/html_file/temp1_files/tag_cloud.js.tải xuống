"use strict";var tag_cloud=function(){$.when(function(a,e){var t=$.Deferred();return $.ajax({url:"/api/tag-cloud",type:"get",cache:!0,timeout:5e3,success:t.resolve,error:t.reject}),t.promise()}()).always(function(a){var e=a;e&&e.map(function(a){var e=a.name,t=a.slug;$(".h-body-navContentTagsList").append($('\n    <li class="h-body-navContentTagsItem"><a href="'+tagPathBase.replace(/______/,t)+'">'+e+"</a></li>\n    "))})})};tag_cloud();